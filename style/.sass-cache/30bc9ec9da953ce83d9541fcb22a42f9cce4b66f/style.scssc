@import "reset";

/* =Global
----------------------------------------------- */
@font-face
{
	font-family: halion;
	src: url('fonts/halion.ttf'),
     	 url('fonts/halion.svg'); /* IE4+ */
}

$black: 		rgba(0,0,0,1);
$white: 		rgba(255,255,255,1);
$near-black:	rgba(30, 30, 30, 1);
$offwhite: 		rgba(255,255,230,1);

@mixin text-blur($radius, $colour) {
	color: transparent;
	text-shadow: 0px 0px $radius $colour;
}

body,
button,
input,
select,
textarea {
	background-color: $near-black;
	color: $offwhite;
	font-size: 2.0em;
	font-family: halion, georgia, serif;
	line-height: 1.5;

	-webkit-transition: all 0.25s ease-in-out;
	-moz-transition: 	all 0.25s ease-in-out;
	-o-transition: 		all 0.25s ease-in-out;
	transition: 		all 0.25s ease-in-out;
}

/* Headings */
h1,h2,h3,h4,h5,h6 {
	clear: both;
}

.colour-toggle {
	position: absolute;
	cursor: pointer;
	img {
		height: 60px;
		fill: $offwhite;

		&.white-switch {
			display:block;
		}
		&.black-switch {
			display:none;
		}
		&:not(.inverted) {
			-webkit-transform: rotate(180deg);
			-moz-transform: rotate(180deg);
			-o-transform: rotate(180deg);
			-ms-transform: rotate(180deg);
			transform: rotate(180deg);
		}
	}
}

/* Site title*/
/* backup title CSS */
.site-title {
	text-align: center;
	font-size: 3.3em;
	letter-spacing: 0.1em;

	a, a:visited, a:active {
		color: $offwhite;
		font-weight: normal;
		font-style: italic;
		text-decoration: none;
		padding: 0px 10px 6px 10px;
	}

	a:hover {
		color: $offwhite;
		text-shadow:none;
		text-decoration: none;
		border-bottom: solid 1px $offwhite;	
	}
}
/* ideal title CSS */
.textshadow .site-title {
	text-align: center;
	font-size: 3.3em;
	font-style: normal;
	letter-spacing: 0.1em;

	a, a:visited, a:active {
		@include text-blur(3px, $offwhite);
		text-decoration: none;
		padding: 0px 10px 6px 10px;
	}

	a:hover {
		color: $offwhite;
		text-shadow:none;
		text-decoration: none;
		border-bottom: solid 1px $offwhite;	
	}
}


/* page dividers */

hr {
	width: 60%;
	background-color: $offwhite;
	border: 0;
	height: 1px;
	margin-bottom: 1.5em;
}

.divide {
	width: 100%;
	height: 1px;
	background-color: $offwhite;
	text-align: center;
	margin: 30px auto 80px auto;
	&.single {
		margin: 80px auto 80px auto;
	}


	.divide-char {
		background-color: $near-black;
		position: relative;
		top: -0.6em;
		padding: 1.0em;
	}
}

.footer-divide {
		width: 65%;
		margin-bottom: 30px;
}

/* Text elements */
p {
	margin-bottom: 1.5em;
}
.amp {
